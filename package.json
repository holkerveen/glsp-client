    "update:next": "yarn upgrade -p \"sprotty|@eclipse-glsp/.*\" --next "
  },
  "devDependencies": {
    "lerna": "^2.11.0",
    "tslint": "^5.5.0",
    "tslint-loader": "^3.4.3",
    "typescript": "^3.9.2"
  },
  "workspaces": [
    "packages/*",
    "examples/*"
  ]
}
